<%- include('../partials/head') -%>
    <%- include('../partials/nav') -%>



        <div class="home-container">


            <div class="login-form-container">
                <main>


                    <form action="/login" method="POST" class="log-in-form">
                        <h1>Log In</h1>

                        <div class="login-container">
                            <label for="username">Username:</label>
                            <input type="text" placeholder="Enter Username" name="username" minlength="3" maxlength="15"
                                required>

                            <label for="password">Password:</label>
                            <input type="password" placeholder="Enter Password" name="password" minlength="4"
                                maxlength="16" required>

                            <button type="submit">LOG IN</button>

                            <button><a href="/auth/google">LOG IN WITH
                                    GOOGLE</a></button>

                            <h1>
                            </h1>
                            <button><a href="signup">SIGN UP</a>
                        </div>
                    </form>
                </main>
            </div>


        </div>



        </body>
        <%- include('../partials/footer') -%>